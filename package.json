{
  "name": "hypatia-app",
  "version": "0.0.12",
  "packageManager": "yarn@3.0.1",
  "devDependencies": {
    "typescript": "^4.4.2"
  },
  "workspaces": [
    "*",
    "private/*"
  ],
  "dependencies": {
    "@hypatia-app/backend": "^0.0.11",
    "@hypatia-app/client": "^0.0.9",
    "node-gyp": "^8.2.0"
  },
  "bin": {
    "hypatia": "./index.js"
  },
  "r": {
    "ts:watch": "tsc -b -w common quill backend",
    "watch": [
      "r:ts:watch",
      "r:backend:watch",
      "r:client:watch"
    ],
    "ts:build": "tsc -b backend",
    "build": [
      "r:ts:build",
      "r:client:build"
    ],
    "start": "r:backend:start"
  },
  "license": "MIT"
}
