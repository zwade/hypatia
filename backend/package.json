{
    "name": "@hypatia-app/backend",
    "version": "0.0.9",
    "main": "./dist/index.js",
    "dependencies": {
        "@hypatia-app/client": "^0.0.7",
        "@zensors/sheriff": "^2.0.2",
        "express": "^4.17.1",
        "express-ws": "^5.0.2",
        "fs-extra": "^10.0.0",
        "node-pty": "^0.10.1"
    },
    "devDependencies": {
        "@types/express": "^4",
        "@types/express-ws": "^3.0.1",
        "@types/fs-extra": "^9.0.12"
    },
    "scripts": {
        "prepublish": "rm -rf ./dist && yarn r ts:build"
    },
    "r": {
        "backend:watch": {
            "watch": [
                "dist"
            ],
            "script": "node dist/index.js"
        },
        "backend:start": "node dist/index.js"
    },
    "resolutions": {
        "@hypatia-app/client": "workspace:*"
    }
}
