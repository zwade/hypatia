{
  "name": "@hypatia-app/quill",
  "version": "0.0.16",
  "packageManager": "yarn@3.0.1",
  "main": "./dist/exports.js",
  "types": "./dist/exports.d.ts",
  "dependencies": {
    "@hypatia-app/common": "^0.0.16",
    "dockerode": "^3.3.1",
    "node-pty": "^0.10.1",
    "portfinder": "^1.0.28"
  },
  "resolutions": {
    "@hypatia-app/common": "workspace:*"
  },
  "scripts": {
    "prepublish": "yarn r ts:build"
  },
  "r": {
    "quill:watch-ts": "tsc -w",
    "quill:watch-node": {
      "script": "node dist/index.js",
      "watch": [
        "./dist",
        "../common/dist",
        "../backend/dist"
      ]
    }
  },
  "devDependencies": {
    "@types/dockerode": "^3"
  }
}